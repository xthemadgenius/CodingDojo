@model DojoSurveyWithValidation.Models.Users
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/styles.css">
    <title>Document</title>
</head>
<body>
    <h1>Summitted Info</h1>

    <div>
        <h3>Name:</h3>
        <p>@Model.Name</p>
    </div>
    <div>
        <h3>Dojo Location</h3>
        <p>@Model.Location</p>
    </div>
    <div>
        <h3>Favorite Language</h3>
        <p>@Model.Language</p>
    </div>
    <div>
        <h3>Comment</h3>
        <p>@Model.Comment</p>
    </div>
    <a href="/">Go Back</a>
</body>
</html>